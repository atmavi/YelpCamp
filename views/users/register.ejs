<% include ../partials/header%>

<div class="container text-center h-100 mx-auto">
	<div class="row align-items-center">
		<h1 class="col-10 col-md-6 col-lg-4 mx-auto">Create new User</h1>
	</div>
	<div class="row align-items-center ">
		<form class="col-10 col-md-6 col-lg-4 mx-auto" action="/register" method="POST">
			<div class="row align-items-center">
				<input class="col-12 text-center m-2" type="text" name="username" placeholder="Username"></input>
				<input class="col-12 text-center m-2" type="password" name="password" placeholder="Password">
			</div>
			<button class="w-25 text-center ">Submit</button>
		</form>
	</div>
</div>

<%include ../partials/footer%>

